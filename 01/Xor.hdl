/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Or(a=a, b=b, out=or_a_b);
    Nand(a=a, b=b, out=nand_a_b);
    And(a=or_a_b, b=nand_a_b, out=out);
}
